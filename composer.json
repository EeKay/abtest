{
    "name": "apurbajnu/abtest",
    "description": "Laravel 8 AB testing package",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Apurbajnu\\Abtest\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Apurba Podder",
            "email": "apurba.jnu@gmail.com"
        }
    ],
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "apurbajnu/abtest", 
                "version": "1.0",
                "source": {
                    "url": "https://github.com/apurbajnu/abtest.git", 
                    "type": "git",
                    "reference": "master" 
                }
            }
        }],
    "require": {
        "php": "^7.4 || ^8.0",
        "laravel/framework": "^5.3 || ^6.0 || ^7.0 || ^8.0 || ^9.0",
        "jaybizzle/crawler-detect": "^1.2",
        "apurbajnu/abtest": "1.0"
    },
    "require-dev": {
        "orchestra/testbench": "3.9.*",
        "phpunit/phpunit": "^8.0 | ^9.0",
        "mockery/mockery": "^1.0"
    },
    "autoload-dev": {
        "psr-4": {
            "apurbajnu\\AbTesting\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "apurbajnu\\AbTesting\\AbTestingServiceProvider"
            ],
            "aliases": {
                "AbTesting": "apurbajnu\\AbTesting\\AbTestingFacade"
            }
        }
    }
}
